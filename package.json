{
	"name": "vscode-multi-line-tricks",
	"displayName": "vscode-multi-line-tricks",
	"description": "Enable Ctrl+Shif+L and Ctrl+L behavior on VSCode to be the same as in Sublime",
	"version": "0.0.1",
	"publisher": "bigous",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:bcjti.selectLine",
		"onCommand:bcjti.breakSelection"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [ ],
		"keybindings": [
			{ "key": "alt+l",             "when": "editorTextFocus", "command": "bcjti.selectLine" },
			{ "key": "shift+alt+l",       "when": "editorTextFocus", "command": "bcjti.breakSelection" }
		]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}